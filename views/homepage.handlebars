{{#each posts as |post|}}
<h1> <a href="/postpage/{{post.id}}">Title: {{post.title}} </a></h2>
    <h2> Content: {{post.content}}</h2>
    <h3> Age: {{post.createdAt}}</h2>
        <h4> User: {{post.user.name}}</h2>
            {{#each comments as |comment|}}
            <h5> Comments: {{comment.content}}
    </h3>
    <h6> Posted by: {{comment.user.name}} on {{comment.createdAt}}.</h4>
        {{/each}}
        {{/each}}